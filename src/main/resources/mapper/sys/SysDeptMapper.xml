<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kc.eh.dao.SysDeptDao">
	<select id="findById"
            resultType="com.kc.eh.entity.SysDept">
           select *
           from sys_depts
           where id=#{id}
    </select>
</mapper>  